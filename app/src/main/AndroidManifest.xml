<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kangkan.developer.resultprocessingsystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Admin_LogIn_Activity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".Admin_register_Activity" />
        <activity
            android:name=".Add_student_Activity"
            android:parentActivityName=".DashboardActivity" />
        <activity
            android:name=".Add_first_semester_result_Activity"
            android:parentActivityName=".DashboardActivity" />
        <activity
            android:name=".Add_second_semester_result_Activity"
            android:parentActivityName=".DashboardActivity" />
        <activity
            android:name=".Show_all_student_Activity"
            android:parentActivityName=".DashboardActivity" />
        <activity
            android:name=".Edit_student_details_Activity"
            android:parentActivityName=".Show_all_student_Activity" />
        <activity
            android:name=".show_result_Activity"
            android:parentActivityName=".WelcomeActivity" />
        <activity
            android:name=".show_result_details_Activity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".Manage_result_Activity" />
        <activity android:name=".Manage_result_2_Activity" />
        <activity android:name=".Edit_result_Activity"></activity>
    </application>

</manifest>